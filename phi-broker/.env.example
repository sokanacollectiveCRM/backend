# PHI Broker Environment Variables

# Service port (Cloud Run sets this automatically)
PORT=8080

# HMAC shared secret for service-to-service auth
# Must match PHI_BROKER_SHARED_SECRET in Vercel backend
PHI_BROKER_SHARED_SECRET=your-shared-secret-here

# Cloud SQL connection (private network)
SENSITIVE_DATABASE_HOST=10.x.x.x
SENSITIVE_DATABASE_PORT=5432
SENSITIVE_DATABASE_NAME=sensitive_db
SENSITIVE_DATABASE_USER=phi_broker
SENSITIVE_DATABASE_PASSWORD=your-password-here
SENSITIVE_DATABASE_SSL=true
